!(function(w, h){
  'use strict';

  h = w.__hermes_embed = {};
  h.assets = {
    logo: "<%= asset_url('hermes-logo.png') %>"
  };
  h.protocol = "<%= ENV['protocol'] || 'http' %>"; // see env/production.rb
  h.host = "<%= compute_asset_host %>";
  h.env = "<%= Rails.env %>";
  h.pagepath = w.location.protocol + h.host;
  h.DOM = {} // shortcut to get HERMES dom nodes (+ cache)

})(this);